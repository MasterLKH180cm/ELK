    },
    {
      "name": "Session Logs",
      "index_pattern": "logs-session-*",
      "time_field": "@timestamp",
      "description": "User session logs"
    },
    {
      "name": "Worklist Logs",
      "index_pattern": "logs-worklist-*",
      "time_field": "@timestamp",
      "description": "Worklist service logs"
    },
    {
      "name": "Viewer Logs",
      "index_pattern": "logs-viewer-*",
      "time_field": "@timestamp",
      "description": "Medical image viewer logs"
    },
    {
      "name": "Default Logs",
      "index_pattern": "logs-default-*",
      "time_field": "@timestamp",
      "description": "Uncategorized logs"
