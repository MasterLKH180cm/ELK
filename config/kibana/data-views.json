      "description": "All logs across all services"
    },
    {
      "name": "Auth Logs",
      "index_pattern": "logs-auth-*",
      "time_field": "@timestamp",
      "description": "Authentication and authorization logs"
    },
    {
      "name": "Frontend Logs",
      "index_pattern": "logs-frontend-*",
      "time_field": "@timestamp",
      "description": "Frontend application logs"
    },
    {
      "name": "Backend Logs",
      "index_pattern": "logs-backend-*",
      "time_field": "@timestamp",
      "description": "Backend application logs"
    },
    {
      "name": "OHIF Logs",
      "index_pattern": "logs-ohif-*",
      "time_field": "@timestamp",
      "description": "OHIF viewer logs"
    },
    {
      "name": "Dictation Backend Logs",
      "index_pattern": "logs-dictation_backend-*",
      "time_field": "@timestamp",
      "description": "Dictation backend service logs"
    },
    {
      "name": "Dictation Frontend Logs",
      "index_pattern": "logs-dictation_frontend-*",
      "time_field": "@timestamp",
      "description": "Dictation frontend logs"
    },
    {
